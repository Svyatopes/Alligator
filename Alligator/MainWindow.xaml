<Window x:Class="Alligator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Alligator"
        xmlns:tabItems="clr-namespace:Alligator.UI.TabItems"
        mc:Ignorable="d"
        Title="АЛЛИГАТОР ЙОУ" Height="450" Width="800">
    <Grid>
        <TabControl TabStripPlacement="Left" >
            <tabItems:TabItemMain />
            <tabItems:TabItemClients />
            <tabItems:TabItemOrders />
            <tabItems:TabItemProducts />
            <tabItems:TabItemCategories />
            <tabItems:TabItemSupplies />
        </TabControl>
    </Grid>
</Window>
